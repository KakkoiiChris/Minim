[] = 1 & "How many stars? ".

_> 0.
    $< [[0]++].
    
    _^ ![[0]].
_< 0.

#> [0].

$< '\n'.

[1] = 0.

_> 1.
    [2] = 0.
    
    _> 2.
        $< '*'.
        
        [2]++.
        
        _^ [2] == [0] - [1].
    _< 2.

    $< '\n'.
    
    [1]++.
    
    _^ [1] == [0].
_< 1.