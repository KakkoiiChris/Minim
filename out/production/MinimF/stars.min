[0] = 1.
[1:] = "How many stars: ".

_> 1.
    $< [[0]].
    
    [0] = [0] + 1.
    
    _< ?[[0]].
_> 0.

#> [0].

$< '\n'.

[1] = 0.

_> 'A'.
    [2] = 0.
    
    _> 'B'.
        $< '*'.
        
        [2] = [2] + 1.
        
        _< 'B' + ([2] == [0] - [1]) * 32.
    _> 'b'.

    $< '\n'.
    
    [1] = [1] + 1.
    
    _< 'A' + ([1] == [0]) * 32.
_> 'a'.

$< '\n'.